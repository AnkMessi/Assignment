#include <stdio.h>

int main() {
  int a, i;
  printf("Enter the number of elements to be in array: ");
  scanf("%d", &a);
  int N[a];
  for (i = 1; i <= a; i++) {
    printf("Enter values of array: ");
    scanf("%d", &N[i]);
  }
  printf("In reverse element: ");
  for (i = a; i > 0; i--) {
    printf("%d ", N[i]);
  }
  return 0;
}
